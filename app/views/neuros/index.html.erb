<p id="notice"><%= notice %></p>

<h1>Team Deathmatch Player List</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <div class="row" id="match_show">
      <div class="col-md-8 col-md-offset-2">
        <div class="panel panel-default">
              <tbody>
                <% @neuros.each do |neuro| %>
                <div class="box panel panel-default matchbox">
                    <div class="panel-body">
                      <%= link_to neuro.playername, neuro %>
                    </div>
                </div>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>

<br>

<div class="col-md-8 col-md-offset-2">
    <%= link_to 'New Player Analysis', new_neuro_path %>
</div>
